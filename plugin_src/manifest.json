{
"name": "AddWord",
"version": "0.0.0.1",
"manifest_version": 2,
"description": "add wordbook to shanbay.com.",
"browser_action": {
"default_icon": "icon.png",
"default_popup": "popup.html"
},
"background": {
    "page": "popup.html"
},
"permissions": [
    "cookies",
    "http://*.shanbay.com/*",
	"tabs"
]
}